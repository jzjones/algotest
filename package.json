    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "jest": "^27.4.7",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.5.5"
  }
}
